{
	"include": [
		"$(MODDABLE)/examples/manifest_base.json",
		"$(MODDABLE)/modules/io/manifest.json"
	],
	"modules": {
		"*": [
			"./main",
			"./resourcestreamer",
			"../calculatePower"
		],
		"pins/*": [
			"$(MODULES)/pins/i2s/*"
		]
	},
	"resources": {
		"*": "../voice_samples/googletts_16bit_mono_11025hz"
	},
	"preload": [
		"resourcestreamer"
	],
	"defines": {
		"audioOut": {
			"queueLength": 24
		}
	},
	"platforms": {
		"mac": {
			"defines": {
				"audioOut": {
					"bitsPerSample": 16,
					"numChannels": 1,
					"sampleRate": 11025
				}
			}
		}
	}
}
